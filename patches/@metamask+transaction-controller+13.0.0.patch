+import type { Hex } from '@metamask/utils';
+export interface EtherscanTransactionMetaBase {
+    blockNumber: string;
+    blockHash: string;
+    confirmations: string;
+    contractAddress: string;
+    cumulativeGasUsed: string;
+    from: string;
+    gas: string;
+    gasPrice: string;
+    gasUsed: string;
+    hash: string;
+    nonce: string;
+    timeStamp: string;
+    to: string;
+    transactionIndex: string;
+    value: string;

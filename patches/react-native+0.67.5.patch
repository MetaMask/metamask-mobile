+                  });
+                },
+              );
+            });
+          });
+        },
+      );
+    });
+  });
+});
+
+function parseModule(source) {
+  const schema = parseString(source, `${MODULE_NAME}.js`);
+  const module = schema.modules.NativeFoo;
+  invariant(
+    module.type === 'NativeModule',
+    "'nativeModules' in Spec NativeFoo shouldn't be null",
+  );
+  return module;

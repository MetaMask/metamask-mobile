+  return filenames.reduce<SchemaType>(
+    (accumulator, file) => {
+      const schema = parser.parseFile(`${FIXTURE_DIR}/${file}`);
+      return {
+        modules: {
+          ...accumulator.modules,
+          ...schema.modules,
+        },
